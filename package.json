    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/platform-socket.io": "^7.6.17",
    "@nestjs/websockets": "^7.6.17",
    "axios": "^1.2.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "config": "^3.3.8",
    "form-data": "^4.0.0",
    "helmet": "^6.0.1",
    "imgbb-uploader": "^1.5.0",
